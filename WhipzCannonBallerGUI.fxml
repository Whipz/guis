<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="213.0" prefWidth="294.0"
            xmlns="http://javafx.com/javafx/11.0.1" fx:controller="scripts.core.cannonballs.WhipzCannonGUIController">
    <children>
        <Label layoutX="35.0" layoutY="14.0" text="Whipz CannonBaller" textFill="RED">
            <font>
                <Font name="System Bold" size="24.0"/>
            </font>
        </Label>
        <Button fx:id="startButton" layoutX="231.0" layoutY="178.0" mnemonicParsing="false"
                onAction="#startButtonPressed" prefHeight="25.0" prefWidth="56.0" text="Start"/>
        <Label layoutX="14.0" layoutY="63.0" text="Restocking Amount:"/>
        <TextField fx:id="restockAmount" layoutX="133.0" layoutY="59.0" prefHeight="25.0" prefWidth="56.0" text="1000"/>
        <CheckBox fx:id="doricsCheckBox" layoutX="133.0" layoutY="91.0" mnemonicParsing="false"/>
        <Label layoutX="14.0" layoutY="91.0" text="Dorics Quest?"/>
        <CheckBox fx:id="knightsCheckBox" layoutX="133.0" layoutY="118.0" mnemonicParsing="false"/>
        <Label layoutX="14.0" layoutY="118.0" text="The Knights Sword?"/>
        <CheckBox fx:id="cannonCheckBox" layoutX="133.0" layoutY="145.0" mnemonicParsing="false"/>
        <Label layoutX="14.0" layoutY="145.0" text="Dwarf Cannon?"/>
        <CheckBox fx:id="levelsCheckBox" layoutX="133.0" layoutY="173.0" mnemonicParsing="false"/>
        <Label layoutX="14.0" layoutY="173.0" text="Get remaining levels"/>
        <Label layoutX="203.0" layoutY="63.0" text="Buy %"/>
        <Label layoutX="203.0" layoutY="91.0" text="Sell %"/>
        <TextField fx:id="buyPercentField" layoutX="245.0" layoutY="59.0" prefHeight="25.0" prefWidth="42.0"
                   text="1.01"/>
        <TextField fx:id="sellPercentField" layoutX="245.0" layoutY="87.0" prefHeight="25.0" prefWidth="42.0"
                   text="0.98"/>
    </children>
</AnchorPane>
