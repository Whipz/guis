<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="262.0" prefWidth="291.0" xmlns="http://javafx.com/javafx/11.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="scripts.core.staker.WhipzStakerGUIController">
   <Label layoutX="30.0" layoutY="14.0" text="Whipz Staker" textFill="RED">
      <font>
         <Font name="System Bold" size="37.0"/>
      </font>
   </Label>
   <Button fx:id="startButton" layoutX="190.0" layoutY="216.0" mnemonicParsing="false" onAction="#startButtonPressed"
           prefHeight="32.0" prefWidth="87.0" text="Start"/>
   <Label layoutX="14.0" layoutY="79.0" text="Initial Stake Amount: " textFill="RED">
      <font>
         <Font name="System Bold" size="12.0"/>
      </font>
   </Label>
   <TextField fx:id="textInitial" layoutX="144.0" layoutY="75.0" prefHeight="25.0" prefWidth="123.0" text="eg.. 1m"/>
   <Label layoutX="14.0" layoutY="126.0" text="Multipler:" textFill="RED">
      <font>
         <Font name="System Bold" size="12.0"/>
      </font>
   </Label>
   <TextField fx:id="textMultipler" layoutX="144.0" layoutY="122.0" prefHeight="25.0" prefWidth="123.0" text="eg.. 2"/>
   <Label layoutX="14.0" layoutY="175.0" text="Stoploss:" textFill="RED">
      <font>
         <Font name="System Bold" size="12.0"/>
      </font>
   </Label>
   <TextField fx:id="textStopLoss" layoutX="144.0" layoutY="171.0" prefHeight="25.0" prefWidth="123.0" text="eg.. 20m"/>
</AnchorPane>
